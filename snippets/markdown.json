{
  "Front Matter": {
    "prefix": "fm",
    "body": [
      "---",
      "title: ${1:标题}",
      "date: $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE",
      "tags: [${2:标签1}, ${3:标签2}]",
      "description: ${4:描述}",
      "---",
      "",
      "$0"
    ],
    "description": "Markdown文档Front Matter"
  },
  "Code Block": {
    "prefix": "code",
    "body": ["```${1:language}", "$0", "```"],
    "description": "代码块"
  },
  "Table": {
    "prefix": "table",
    "body": [
      "| ${1:列1} | ${2:列2} | ${3:列3} |",
      "| --- | --- | --- |",
      "| ${4:内容1} | ${5:内容2} | ${6:内容3} |$0"
    ],
    "description": "表格"
  },
  "Image": {
    "prefix": "img",
    "body": ["![${1:替代文本}](${2:图片URL})$0"],
    "description": "图片"
  },
  "Link": {
    "prefix": "link",
    "body": ["[${1:链接文本}](${2:URL})$0"],
    "description": "链接"
  },
  "Task List": {
    "prefix": "task",
    "body": [
      "- [${1| ,x|}] ${2:任务1}",
      "- [${3| ,x|}] ${4:任务2}",
      "- [${5| ,x|}] ${6:任务3}$0"
    ],
    "description": "任务列表"
  },
  "Collapsible Section": {
    "prefix": "details",
    "body": [
      "<details>",
      "<summary>${1:标题}</summary>",
      "",
      "${2:内容}",
      "",
      "</details>$0"
    ],
    "description": "可折叠区域"
  }
}
